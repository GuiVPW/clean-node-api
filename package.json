{
  "name": "clean-node-api",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "dev": "env-cmd tsnd --respawn --transpile-only --ignore-watch node_modules --no-notify src/main/server.ts",
    "start": "env-cmd node dist/server.js",
    "test": "env-cmd jest --passWithNoTests --silent --runInBand --detectOpenHandles",
    "test:verbose": "env-cmd jest --passWithNoTests --runInBand",
    "test:unit": "env-cmd yarn test --watch -c jest-unit.config.ts",
    "test:integration": "env-cmd yarn test --watch -c jest-integration.config.ts",
    "test:staged": "env-cmd yarn test --findRelatedTests",
    "test:ci": "env-cmd yarn test --coverage"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "fast-glob": "^3.2.4",
    "mongodb": "^3.6.3",
    "validator": "^13.5.2"
  }
}
